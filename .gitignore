# ===============================================
# Sci-Bono Clubhouse LMS - Git Ignore File
# ===============================================
# This file prevents sensitive data and unnecessary files
# from being committed to version control.
#
# SECURITY CRITICAL: This file protects secrets and API keys
# ===============================================

# ===============================================
# ENVIRONMENT & CONFIGURATION FILES (CRITICAL!)
# ===============================================
# Never commit environment files with real credentials
.env
.env.local
.env.production
.env.staging
.env.development
.env.backup
.env.*.php
.env.*.local

# Keep .env.example for documentation purposes
!.env.example

# Configuration files with credentials
config/database.php.local
config/credentials.php
config/secrets.php
server.php.local

# ===============================================
# LOGS & DEBUG FILES (CRITICAL!)
# ===============================================
# Log files often contain sensitive data:
# - Session tokens
# - API keys
# - User credentials
# - Personal information
# - Error traces with internal paths

# Application logs
storage/logs/*.log
storage/logs/*.txt
storage/logs/**/*.log
storage/logs/**/*.txt
logs/*.log
logs/**/*.log

# Debug logs
debug.log
error.log
access.log
*.log

# PHP error logs
php_errors.log
php-error.log

# ===============================================
# CACHE & TEMPORARY FILES
# ===============================================
# Cache files can contain serialized session data
storage/cache/*
storage/cache/**/*
!storage/cache/.gitkeep

# Temporary files
storage/tmp/*
storage/tmp/**/*
!storage/tmp/.gitkeep

# Session files
storage/sessions/*
storage/sessions/**/*
!storage/sessions/.gitkeep

# Compiled views/templates
storage/views/*
storage/framework/cache/*
storage/framework/sessions/*
storage/framework/views/*

# ===============================================
# UPLOADS & USER-GENERATED CONTENT
# ===============================================
# User uploads may contain sensitive documents
# Uncomment if you want to exclude uploads from git
# public/assets/uploads/*
# public/assets/uploads/**/*
# !public/assets/uploads/.gitkeep

# Exclude specific sensitive upload types
public/assets/uploads/**/*.pdf
public/assets/uploads/**/*.doc
public/assets/uploads/**/*.docx
public/assets/uploads/**/*.xls
public/assets/uploads/**/*.xlsx
public/assets/uploads/documents/*
public/assets/uploads/private/*

# ===============================================
# BACKUP FILES
# ===============================================
*.sql
*.sql.gz
*.sql.zip
*.backup
*.bak
*.old
*.orig
*~
*.swp
*.swo
*.tmp

# Database dumps
database/dumps/*
database/backups/*
*.dump

